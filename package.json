{
  "name": "@lit-protocol/flags",
  "version": "1.0.3",
  "license": "MIT",
  "packageManager": "pnpm@10.7.0",
  "repository": "https://github.com/LIT-Protocol/lit-flags",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "^20.11.1",
    "pnpm": "10.7.0"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "lit-flags": "dist/bin/flag-editor.cjs"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "types": "dist/index.d.mts",
  "scripts": {
    "lint": "eslint",
    "build": "tsc --noEmit && unbuild --parallel",
    "prepare": "husky",
    "prepublishOnly": "eslint && pnpm build",
    "flags": "bun ./src/bin/flag-editor.ts",
    "test": "jest",
    "clean": "rm -Rf dist node_modules",
    "ci:publish": "pnpm publish -r"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.3",
    "@eslint/js": "^9.26.0",
    "@inquirer/prompts": "^7.5.0",
    "@inquirer/testing": "^2.1.46",
    "@tsconfig/node20": "^20.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.30",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "bun": "^1.2.12",
    "cli-table3": "^0.6.5",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import-x": "^4.11.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-perfectionist": "^4.12.3",
    "fs-extra": "^11.3.0",
    "globals": "^16.0.0",
    "husky": "^9.0.11",
    "ini": "^5.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "prettier-plugin-jsdoc": "^1.3.2",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0",
    "unbuild": "^3.5.0",
    "yargs": "^17.7.2"
  },
  "unbuild": {
    "failOnWarn": false
  }
}
